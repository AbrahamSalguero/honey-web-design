//Here can change all the parameters for the sliders created with Swiper.js
const swiper = new Swiper('.swiper', {
  // Optional parameters
  direction: 'horizontal',
  loop: true,
  
  //autoplay definition
   autoplay: {
   delay: 5000,
   disableOnInteraction:false,
 },

  // Navigation arrows
  navigation: {
    nextEl: '.swiper-button-next',
    prevEl: '.swiper-button-prev',
  },
});
